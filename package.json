  "private": true,
  "dependencies": {
    "@fluentui/react": "^8.110.7",
    "@fluentui/react-card": "^9.0.18",
    "@fluentui/react-components": "^9.23.1",
    "@fluentui/react-icons": "^2.0.204",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.18.53",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
